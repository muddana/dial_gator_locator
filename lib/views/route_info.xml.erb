<?xml version="1.0" encoding="UTF-8" ?>
<Response>
  <Gather action="/calls/route_info" method="GET" timeout="10">
    <Say voice="man">Route number <%=  params[:Digits]  %> . <%=  @route_name %></Say>
    <%@busses.each do |bus|%>
      <Say voice="man"><%= bus.to_s %></Say>
    <%end%>
     <Pause/>
    <Say voice="woman">Please press the rts-route number followed by the pound key or please hangup.</Say>
  </Gather>
</Response>
